    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "WallE\\LaravelImageresize\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rahman-Shaikat",
            "email": "shaikat3257@gmail.com"
        }
